@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix : <http://mapping.example.com/>.
@prefix ex: <http://example.com/ex/>.

:map_m1_000 rml:logicalSource :source_000.
:source_000 a rml:LogicalSource;
    rml:source "f1.csv";
    rml:referenceFormulation ql:CSV.
:map_m1_000 a rr:TriplesMap;
    rdfs:label "m1".
:s_000 a rr:SubjectMap.
:map_m1_000 rr:subjectMap :s_000.
:s_000 rr:template "http://example.com/ex/{Id}".
:pom_000 a rr:PredicateObjectMap.
:map_m1_000 rr:predicateObjectMap :pom_000.
:pm_000 a rr:PredicateMap.
:pom_000 rr:predicateMap :pm_000.
:pm_000 rr:constant ex:myProperty.
:pom_000 rr:objectMap :om_000.
:om_000 a rr:ObjectMap;
    rr:constant "http://example.com/ex/myObject";
    rr:termType rr:IRI.
