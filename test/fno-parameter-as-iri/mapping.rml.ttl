@prefix fno: <http://w3id.org/function/ontology#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix : <http://mapping.example.com/>.
@prefix ex: <http://example.com/>.
@prefix dyversify: <http://example.com/idlab/functions/dyversify/>.
@prefix idlab-fn: <http://example.com/idlab/function/>.

:map_metadata_0 rml:logicalSource :source_0.
:source_0 a rml:LogicalSource;
    rml:source "api-metadata.json";
    rml:iterator "$";
    rml:referenceFormulation ql:JSONPath.
:map_metadata_0 a rr:TriplesMap;
    rdfs:label "metadata".
:s_0 a rr:SubjectMap.
:map_metadata_0 rr:subjectMap :s_0.
:s_0 rr:template "http://example.com/{id}".
:pom_0 a rr:PredicateObjectMap.
:map_metadata_0 rr:predicateObjectMap :pom_0.
:pm_0 a rr:PredicateMap.
:pom_0 rr:predicateMap :pm_0.
:pm_0 rr:constant rdf:type.
:pom_0 rr:objectMap :om_0.
:om_0 a rr:ObjectMap;
    rr:constant "http://example.com/Sensor";
    rr:termType rr:IRI.
:pom_1 a rr:PredicateObjectMap.
:map_metadata_0 rr:predicateObjectMap :pom_1.
:pm_1 a rr:PredicateMap.
:pom_1 rr:predicateMap :pm_1.
:pm_1 rr:constant ex:test.
:pom_1 rr:objectMap :om_1.
:om_1 a fnml:FunctionTermMap;
    rr:termType rr:Literal;
    fnml:functionValue :fn_0.
:fn_0 rml:logicalSource :source_0;
    rr:predicateObjectMap :pomexec_0.
:pomexec_0 rr:predicateMap :pmexec_0.
:pmexec_0 rr:constant fno:executes.
:pomexec_0 rr:objectMap :omexec_0.
:omexec_0 rr:constant "http://example.com/idlab/functions/dyversify/getSensorClasses";
    rr:termType rr:IRI.
:fn_0 rr:predicateObjectMap :pom_2.
:pom_2 a rr:PredicateObjectMap;
    rr:predicateMap :pm_2.
:pm_2 a rr:PredicateMap;
    rr:constant idlab-fn:list.
:pom_2 rr:objectMap :om_2.
:om_2 a rr:ObjectMap;
    rml:reference "allMetrics[*].id";
    rr:termType rr:IRI.
